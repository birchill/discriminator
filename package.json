{
  "name": "discriminator",
  "version": "0.1.0",
  "description": "A tagged union type for superstruct based on JSON typedef's discriminator type",
  "repository": "https://github.com/birchill/discriminator.git",
  "author": "Birchill, Inc.",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/birchill/discriminator/issues"
  },
  "files": [
    "dist"
  ],
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "homepage": "https://github.com/birchill/discriminator#readme",
  "engines": {
    "node": ">=12.0.0"
  },
  "scripts": {
    "build": "tsc",
    "prepublishOnly": "pinst --disable && tsc",
    "postinstall": "husky install",
    "postpublish": "pinst --enable",
    "release": "standard-version",
    "test": "mocha"
  },
  "lint-staged": {
    "*.{js,ts,md}": [
      "prettier --write"
    ]
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "es5",
    "bracketSpacing": true
  },
  "devDependencies": {
    "@types/assert": "^1.5.6",
    "@types/mocha": "^9.0.0",
    "husky": "^7.0.2",
    "lint-staged": "^11.1.2",
    "mocha": "^9.1.0",
    "pinst": "^2.1.6",
    "prettier": "^2.3.2",
    "standard-version": "^9.3.1",
    "superstruct": "^0.15.2",
    "ts-node": "^10.2.1",
    "typescript": "^4.3.5"
  },
  "peerDependencies": {
    "superstruct": "^0.15.0"
  }
}
