{
  "name": "@birchill/discriminator",
  "version": "0.2.0",
  "description": "A tagged union type for superstruct based on JSON typedef's discriminator type",
  "keywords": [
    "superstruct",
    "tagged union",
    "discriminated union",
    "sum type",
    "JSON typedef"
  ],
  "repository": "https://github.com/birchill/discriminator.git",
  "author": "Birchill, Inc.",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/birchill/discriminator/issues"
  },
  "files": [
    "dist"
  ],
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "typings": "dist/index.d.ts",
  "homepage": "https://github.com/birchill/discriminator#readme",
  "engines": {
    "node": ">=12.0.0"
  },
  "scripts": {
    "build": "rollup -c",
    "prepublishOnly": "pinst --disable && rollup -c",
    "postinstall": "husky install",
    "postpublish": "pinst --enable",
    "release": "standard-version",
    "test": "cross-env TS_NODE_COMPILER_OPTIONS='{ \"module\": \"commonjs\", \"target\": \"es2019\" }' ts-mocha -p ./tsconfig.json"
  },
  "lint-staged": {
    "*.{js,ts,md}": [
      "prettier --write"
    ]
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "es5",
    "bracketSpacing": true
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^8.5.0",
    "@types/assert": "^1.5.6",
    "@types/mocha": "^9.0.0",
    "cross-env": "^7.0.3",
    "husky": "^7.0.2",
    "lint-staged": "^11.1.2",
    "mocha": "^9.1.0",
    "pinst": "^2.1.6",
    "prettier": "^2.3.2",
    "rollup": "^2.79.0",
    "rollup-plugin-cleaner": "^1.0.0",
    "rollup-plugin-dts": "^4.2.2",
    "standard-version": "^9.3.1",
    "superstruct": "^0.15.2",
    "ts-mocha": "^10.0.0",
    "ts-node": "^10.2.1",
    "typescript": "^4.3.5"
  },
  "peerDependencies": {
    "superstruct": ">0.16.0 < 2.0.0"
  }
}
